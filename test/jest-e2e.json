{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "moduleNameMapper": {
    "^@prisma": "<rootDir>/prisma",
    "^@logger": "<rootDir>/src/logger",
    "^@middlewares": "<rootDir>/src/middlewares"
  },
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
